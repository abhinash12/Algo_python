   "source": [
    "def rotate(matrix):\n",
    "    l = len(matrix)\n",
    "    m = len(matrix[0])\n",
    "    new_matrix = [[None] * l for _ in range(m)]\n",
    "    for i in range(l):\n",
    "        for j in range(m):\n",
    "            new_matrix[j][l-i-1] = matrix[i][j]\n",
    "    return new_matrix\n",
    "\n"
   ]
